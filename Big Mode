#include<bits/stdc++.h>
using namespace std;

int bmod(int a, int b, int m)
{
    if(b==0) return 1;
    int x= bmod(a,b/2,m);
    x = (x*x)%m;
    if(b%2==1) x = (x*a)%m;
    return x;
}

int main()
{
    int a,b,m; cin>> a >> b >> m;
    cout<< bmod(a,b,m) << endl;

    return 0;
}
